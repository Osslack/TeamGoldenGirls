<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="endScreen" alignment="CENTER" prefWidth="300.0" spacing="25.0" xmlns="http://javafx.com/javafx/8.0.40"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sample.controller.EndScreenController">
    <children>
        <Button fx:id="nextButton" defaultButton="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                prefHeight="35.0" text="Next Level"/>
        <Button fx:id="highscoreButton" cancelButton="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                prefHeight="35.0" text="Highscores"/>
    </children>
    <padding>
        <Insets bottom="15.0" left="10.0" right="10.0" top="15.0"/>
    </padding>
</VBox>
