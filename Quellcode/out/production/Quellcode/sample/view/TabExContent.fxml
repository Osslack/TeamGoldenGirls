<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.40"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.controller.menu.highscore.TabExController">
    <children>
        <TableView fx:id="table" prefHeight="400.0" prefWidth="800.0">
            <columns>
                <TableColumn fx:id="userCol" editable="false" prefWidth="237.0" resizable="false" text="Username"/>
                <TableColumn fx:id="classCol" editable="false" prefWidth="192.0" resizable="false" text="Class"/>
                <TableColumn fx:id="scoreCol" editable="false" prefWidth="150.0" resizable="false" text="Score"/>
                <TableColumn fx:id="dateCol" editable="false" prefWidth="220.0" resizable="false" text="Date"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
