<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.66" xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="sample.controller.DiffController">
    <center>
        <GridPane hgap="10.0" vgap="35.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Easy" />
                <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Medium" GridPane.rowIndex="1" />
                <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Hard" GridPane.rowIndex="2" />
                <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="EXTREME" GridPane.rowIndex="3" />
                <Label text="Beschreibung" GridPane.columnIndex="1" />
                <Label text="Beschreibung" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Label text="Beschreibung" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <Label text="Beschreibung" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            </children>
            <BorderPane.margin>
                <Insets bottom="175.0" left="250.0" right="250.0" top="150.0" />
            </BorderPane.margin>
        </GridPane>
    </center>
    <bottom>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Back" />
                    </children>
                    <GridPane.margin>
                        <Insets left="50.0" />
                    </GridPane.margin>
                </HBox>
            </children>
            <BorderPane.margin>
                <Insets bottom="25.0" />
            </BorderPane.margin>
        </GridPane>
    </bottom>
</BorderPane>
